{
  "name": "diambars-sublim-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "reset": "expo start --clear"
  },
  "expo": {
    "name": "Diambars-Sublim-Mobile",
    "slug": "diambars-sublim-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#040DBF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./src/assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#040DBF",
        "tabletImage": "./src/assets/logo.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo.png",
        "backgroundColor": "#040DBF"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "image": "./src/assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#040DBF",
        "mdpi": "./src/assets/logo.png",
        "hdpi": "./src/assets/logo.png",
        "xhdpi": "./src/assets/logo.png",
        "xxhdpi": "./src/assets/logo.png",
        "xxxhdpi": "./src/assets/logo.png"
      }
    },
    "web": {
      "splash": {
        "image": "./src/assets/logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#040DBF"
      }
    }
  },
  "dependencies": {
    "@expo/metro-config": "^0.18.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "axios": "^1.11.0",
    "expo": "~52.0.0",
    "expo-image-picker": "~16.0.6",
    "expo-status-bar": "~2.0.0",
    "metro": "^0.81.0",
    "phosphor-react-native": "^3.0.1",
    "react": "18.3.1",
    "react-hook-form": "^7.62.0",
    "react-native": "0.76.1",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^4.12.0",
    "react-native-screens": "~4.1.0",
    "react-native-toast-message": "^2.3.3",
    "sweetalert2": "^11.26.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  },
  "private": true
}
